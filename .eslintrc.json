{
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": false,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "arrow-parens": ["error", "as-needed"]
  },
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y", "import"]
}
